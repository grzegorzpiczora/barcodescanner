.val());\n    });\n    $('.input-number').change(function() {\n        \n        minValue =  parseInt($(this).attr('min'));\n        maxValue =  parseInt($(this).attr('max'));\n        valueCurrent = parseInt($(this).val());\n        \n        name = $(this).attr('name');\n        if(valueCurrent >= minValue) {\n            $(\".btn-number[data-type='minus'][data-field='\"+name+\"']\").removeAttr('disabled')\n        } else {\n            alert('Sorry, the minimum value was reached');\n            $(this).val($(this).data('oldValue'));\n        }\n        if(valueCurrent <= maxValue) {\n            $(\".btn-number[data-type='plus'][data-field='\"+name+\"']\").removeAttr('disabled')\n        } else {\n            alert('Sorry, the maximum value was reached');\n            $(this).val($(this).data('oldValue'));\n        }\n    \n    });\n    $(\".input-number\").keydown(function (e) {\n            // Allow: backspace, delete, tab, escape, enter and .\n            if ($.inArray(e.keyCode, [46, 8, 9, 27, 13, 190]) !== -1 ||\n                 // Allow: Ctrl+A\n                (e.keyCode == 65 && e.ctrlKey === true) || \n                 // Allow: home, end, left, right\n                (e.keyCode >= 35 && e.keyCode <= 39)) {\n                     // let it happen, don't do anything\n                     return;\n            }\n            // Ensure that it is a number and stop the keypress\n            if ((e.shiftKey || (e.keyCode < 48 || e.keyCode > 57)) && (e.keyCode < 96 || e.keyCode > 105)) {\n                e.preventDefault();\n            }\n        });\n    \n    // Force Click Event after Find button was clicked\n    $('.lookup_salesforce_record').on( \"click\", function() {\n        $('.scan_and_search_for_salesforce_record').trigger(\"click\");\n\n    });\n    \n\n    //Listen to changes on the text field \n    window.formyoula.form_fields['ca24-7c59-0e82'].on('all', function(){\n        //Set the number field value to 0 when the text field value is changed\n        if(window.formyoula.form_fields['ca24-7c59-0e82'].get('value') > 0) {\n            $('.quantityUpdate .input-number').prop('disabled', false);\n            $('.quantityUpdate .btn-danger').prop('disabled', false);\n            $('.quantityUpdate .btn-success').prop('disabled', false);\n        } else {\n            $('.quantityUpdate .input-number').prop('disabled', true);\n            $('.quantityUpdate .btn-danger').prop('disabled', true);\n            $('.quantityUpdate .btn-success').prop('disabled', true);\n        }\n        // Aktualny Stan Readonly\n        $('.ca24-7c59-0e82').prop('readonly', true);\n    });\n  \n    // Reset to 0 quantity field value\n    $('#component-3560-6293-bbc5').on(\"click\",\".delete_field_value_button\",  function() {\n        $('.ca24-7c59-0e82').val(0).change();\n        $('#component-3d24-225a-e949 .input-number').val(0).trigger(\"change\");\n    });\n\n    // Readonly to Aktualny Stan\n    $('.ca24-7c59-0e82').prop('readonly', true);\n\n\n \n\n\n\n\n    function reset_values() {\n        // 2d8b-b28f-b5ae component id of lookup field\n        formyoula.form_fields[ '3560-6293-bbc5' ].set( {\n          name: '',\n          value: ''\n        } );\n        // ec10-3900-c0a8 component id of number or text field\n        // Set value to zero\n        formyoula.form_fields[ 'ca24-7c59-0e82' ].set( {\n          value: '0'\n        } );\n        // e191-9320-ce5a component id of number or text field\n        // Set value to zero\n \n      }"
